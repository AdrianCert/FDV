<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewBag.Title - Gfest</title>
    <link rel="stylesheet" href="~/Content/bootstrap.4.1.1.min.css">
    <link rel="manifest" href="~/wwwroot/manifest.json">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lato:100i,300,300i,400,700,900">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.1.1/aos.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.8.2/css/lightbox.min.css">
    <link rel="stylesheet" href="~/Content/gfest/styles_gfest.min.css">
</head>

<body style="width: 300px;">
    <div id="wrapper">
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand"> <a href="#">Home </a></li>
                <li> <a href="/profile.html">Profile</a></li>
                <li> <a href="/events.html">Events</a></li>
                <li> <a class="invisible" href="#">LockDown</a></li>
            </ul>
        </div>
        <div class="page-content-wrapper">
            <div class="container-fluid">
            </div>
        </div>
        <a class="btn btn-link" role="button" id="menu-toggle" href="#menu-toggle"><i class="fa fa-bars"></i></a><img class="img-fluid mx-auto d-block;" style="align-items: inherit;width: 50px;" src="~/wwwroot/images/gfest/icon.png">
    </div>
    @RenderBody()
    <script src="~/Scripts/bootstrap.4.3.1 .min.js"></script>
    <script src="~/Scripts/jquery-2.0.0.min.js"></script>
    <script src="~/Scripts/gfest.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.1.1/aos.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.8.2/js/lightbox.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.js"></script>
    <script src="~/Scripts/gfest.min.js"></script>
    @RenderSection("scripts", required: false)
</body>

</html>